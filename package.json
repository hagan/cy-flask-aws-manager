{
  "name": "awsmgr",
  "version": "0.0.1",
  "description": "Launch AWS Manager",
  "scripts": {
    "build": "run-script-os",
    "build:win32": "SET NODE_ENV=production && webpack --progress --color --optimization-minimize && npm run flask-static-digest",
    "builddev": "NODE_ENV=development webpack --progress --color && npm run flask-static-digest",
    "build:default": "NODE_ENV=production webpack --progress --color --optimization-minimize && npm run flask-static-digest",
    "start": "run-script-os",
    "start:win32": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run webpack-watch:win32\" \"npm run flask-server\"",
    "start:default": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run webpack-watch\" \"npm run flask-server\"",
    "webpack-watch": "run-script-os",
    "webpack-watch:win32": "SET NODE_ENV=debug && webpack --mode development --watch",
    "webpack-watch:default": "NODE_ENV=debug webpack --mode development --watch",
    "flask-server": ". $(poetry env info -p)/bin/activate && FLASK_APP='awsmgr.app' flask run --host=0.0.0.0 --port=8080 --extra-files=\"/Users/hagan/projects/active/de-appstream-dev/image/flask-app/src/flask_app/static/build/\"",
    "flask-static-digest": ". $(poetry env info -p)/bin/activate && FLASK_APP='awsmgr.app' flask digest compile",
    "lint": "eslint \"assets/js/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hagan/cy-flask-aws-manager.git"
  },
  "author": "James Dean",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "bugs": {
    "url": "https://github.com/hagan/cy-flask-aws-manager.git/issues"
  },
  "homepage": "https://github.com/hagan/cy-flask-aws-manager.git#readme",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.16",
    "@fortawesome/fontawesome-free": "6.4.2",
    "@mui/icons-material": "^5.15.4",
    "@mui/joy": "^5.0.0-beta.22",
    "@popperjs/core": "2.11.8",
    "axios": "^1.5.1",
    "bootstrap": "5.3.2",
    "htm": "^3.1.1",
    "jquery": "3.7.1",
    "next": "^14.0.4",
    "numeral": "^2.0.6",
    "preact": "^10.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@storybook/addon-essentials": "^7.6.8",
    "@storybook/addon-interactions": "^7.6.8",
    "@storybook/addon-links": "^7.6.8",
    "@storybook/blocks": "^7.6.8",
    "@storybook/nextjs": "^7.6.8",
    "@storybook/react": "^7.6.8",
    "@storybook/test": "^7.6.8",
    "babel-loader": "9.1.3",
    "chokidar": "3.5.3",
    "concurrently": "8.2.1",
    "css-loader": "6.8.1",
    "eslint": "8.49.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-next": "^14.0.4",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-storybook": "^0.6.15",
    "hook-shell-script-webpack-plugin": "^0.3.1",
    "less": "4.2.0",
    "less-loader": "11.1.3",
    "mini-css-extract-plugin": "2.7.6",
    "prettier": "^3.2.2",
    "run-script-os": "1.1.6",
    "storybook": "^7.6.8",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  }
}
